mFrSrt(file_write,num_is) FILE_SRT -> FILE_EN, FILE_NUM_EN   
mTranslate() FILE_EN -> FILE_RU  
mMerge() FILE_NUM_EN, FILE_RU -> FILE_NUM_EN_RU  
m2table() FILE_NUM_EN_RU -> FILE_HTML  
